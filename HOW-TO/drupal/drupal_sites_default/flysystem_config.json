{
  "repo-bin": {
    "driver": "dgi_s3",
    "config": {
      "region": "$S3_BUCKET_REGION",
      "bucket": "$S3_BUCKET_NAME",
      "public": false,
      "prefix": "repo-bin",
      "options": {
        "StorageClass": "INTELLIGENT_TIERING"
      },
      "protocol": "https"
    },
    "cache": true
  },
  "repo-meta": {
    "driver": "dgi_s3",
    "config": {
      "region": "$S3_BUCKET_REGION",
      "bucket": "$S3_BUCKET_NAME",
      "public": false,
      "prefix": "repo-meta",
      "options": {
        "StorageClass": "STANDARD"
      },
      "protocol": "https"
    },
    "cache": true
  },
  "repo-service": {
    "driver": "dgi_s3",
    "config": {
      "region": "$S3_BUCKET_REGION",
      "bucket": "$S3_BUCKET_NAME",
      "public": false,
      "prefix": "repo-service",
      "options": {
        "StorageClass": "INTELLIGENT_TIERING"
      },
      "protocol": "https"
    },
    "cache": true
  }
}
